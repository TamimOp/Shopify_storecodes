{{ 'configurator-woonserre.css' | asset_url | stylesheet_tag }}
<section id="section-number-1" class="section-number-1 s-configurator-container step-1-visible" data-visiblestep="1">
  <div class="c-content s-configurator-container__content">
    {% if section.settings.show_help_button %}
    <div id="vpc-save" class="vpc-save-popup">
      <button type="button" class="vpc-save-popup__button">
        <span class="vpc-save-popup__button-icon">?</span>
        <span class="vpc-save-popup__button-label">Hulp bij de configurator?</span>
      </button>
      <div class="vpc-save-popup__overlay"></div>
      <div class="vpc-save-popup__inner">
        <button class="vpc-save-popup__close-button">
          <svg class="vpc-save-popup__close-button-img" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <line x1="18" y1="6" x2="6" y2="18"></line>
            <line x1="6" y1="6" x2="18" y2="18"></line>
          </svg>
        </button>
        <div class="vpc-help-popup__content">
          <h2 class="vpc-help-popup__title">In 3 stappen je woonserre ontwerpen</h2>
          <div class="vpc-help-popup__steps">
            <div class="vpc-help-popup__step">
              <div class="vpc-help-popup__step-icon">
                <img src="https://cdn.shopify.com/s/files/1/0970/8562/0607/files/maatwerk.svg?v=1767429028" alt="Determine dimensions" width="60" height="60">
              </div>
              <h3 class="vpc-help-popup__step-title">Bepaal de afmetingen</h3>
              <p class="vpc-help-popup__step-description">Kleiner of groter, voer de gewenste afmetingen in voor jouw woonserre. Let wel op dat je dit in centimeters doet.</p>
            </div>
            <div class="vpc-help-popup__step">
              <div class="vpc-help-popup__step-icon">
                <img src="https://cdn.shopify.com/s/files/1/0970/8562/0607/files/configurator.svg?v=1767429015" alt="Choose options" width="60" height="60">
              </div>
              <h3 class="vpc-help-popup__step-title">Kies je opties</h3>
              <p class="vpc-help-popup__step-description">Voor zowel de binnen- als de buitenzijde. Van kozijn tot zijwanden, van verlichting tot afwerking. Stap voor stap geef je jouw voorkeuren op.</p>
            </div>
            <div class="vpc-help-popup__step">
              <div class="vpc-help-popup__step-icon">
                <img src="https://cdn.shopify.com/s/files/1/0970/8562/0607/files/Ontvang-de-offerte.svg?v=1767779429" alt="Receive quote" width="60" height="60">
              </div>
              <h3 class="vpc-help-popup__step-title">Ontvang je offerte</h3>
              <p class="vpc-help-popup__step-description">Alles ingevuld? Al je wensen opgegeven? Dan ontvang je jouw offerte. Indien gewenst maken we een afspraak en nemen we alles samen door.</p>
            </div>
          </div>
          <button class="vpc-help-popup__start-button vpc-save-popup__start-button">
            Start met ontwerpen
          </button>
        </div>
      </div>
    </div>
    {% endif %}
    <div id="vpc-value-popup" class="vpc-value-popup">
      <div class="vpc-value-popup__overlay"></div>
      <div class="vpc-value-popup__inner">
        <button class="vpc-value-popup__close-button" onclick="VPCWoonserreConfigurator.closeValuePopup()">
          <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <line x1="18" y1="6" x2="6" y2="18"></line>
            <line x1="6" y1="6" x2="18" y2="18"></line>
          </svg>
        </button>
        <h3 class="vpc-value-popup__title">Vul je gegevens in en zie direct je geschatte waardevermeerdering!</h3>
        <div class="vpc-value-popup__form-container">
          <div class="vpc-value-popup__form-inputs">
            <input type="text" id="value-postcode" placeholder="Postc." class="vpc-value-popup__input">
            <input type="text" id="value-housenumber" placeholder="Huisnr." class="vpc-value-popup__input">
            <input type="text" id="value-addition" placeholder="Toev." class="vpc-value-popup__input">
            <div class="vpc-value-popup__result">
              <span class="vpc-value-popup__result-label">Meerwaarde:</span>
              <span class="vpc-value-popup__result-value">€0<sup>?</sup></span>
            </div>
          </div>
          <p class="vpc-value-popup__address">Straatnaam, Plaats</p>
        </div>
      </div>
    </div>
    <div id="vpc-container" class="o-wrap VPC_Modern_Skin modern-sticky" data-curr="€"
         data-pricing='{
           "pricePerM2_tier1": {{ section.settings.price_per_m2_tier1 | default: "5550.00" }},
           "pricePerM2_tier2": {{ section.settings.price_per_m2_tier2 | default: "925.00" }},
           "pricePerM2_tier3": {{ section.settings.price_per_m2_tier3 | default: "1725.00" }},
           "pricePerM2_tier4": {{ section.settings.price_per_m2_tier4 | default: "1458.33" }},
           "tier1Max": {{ section.settings.tier1_max | default: "9" }},
           "tier2Max": {{ section.settings.tier2_max | default: "12" }},
           "tier3Max": {{ section.settings.tier3_max | default: "16" }},
           "defaultOptionsOffset": {{ section.settings.default_options_offset | default: "2000" }}
         }'>
      <div id="vpc-modern-skin--prefabriek">
        <div class="brick-loader">
          <div class="brick-loader__inner">
            {% for i in (1..10) %}
              <div class="brick-loader__brick"></div>
            {% endfor %}
            <div class="brick-loader__text">Aanvraag behandelen</div>
          </div>
        </div>
        {% comment %} Sidebar {% endcomment %}
        <div class="vpc-sidebar" id="vpc-sidebar">
          <div class="vpc-sidebar__inner" id="vpc-sidebar__inner">
            <div class="vpc-sidebar__top">
              {% comment %} Size Calculator {% endcomment %}
              <div class="vpc-component vpc-size-calculator addon-component">
                <div class="vpc-addon-header">
                  <div class="vpc-component-title">
                    <h2 class="vpc-component-name heading-four">Grootte van jouw woonserre</h2>
                  </div>
                </div>
                <div class="vpc-addon-options">
                  <input type="hidden" id="extension-size-hidden" value="9" name="extension-size" required>
                  <div class="input-group">
                    <input type="number"
                           name="extension-depth"
                           id="extension-depth"
                           min="{{ section.settings.min_depth }}"
                           max="{{ section.settings.max_depth }}"
                           value="{{ section.settings.default_depth }}"
                           inputmode="numeric"
                           pattern="[0-9]*"
                           class="validate vpc-required-field"
                           maxlength="4"
                           required>
                    <label for="extension-depth">Diepte (cm)</label>
                  </div>
                  <div class="input-group">
                    <input type="number"
                           name="extension-length"
                           id="extension-length"
                           min="{{ section.settings.min_width }}"
                           max="{{ section.settings.max_width }}"
                           value="{{ section.settings.default_width }}"
                           inputmode="numeric"
                           pattern="[0-9]*"
                           class="validate vpc-required-field"
                           maxlength="4"
                           required>
                    <label for="extension-length">Breedte (cm)</label>
                  </div>
                  <div class="extension-size">
                    <span class="extension-size__result">9</span> m<sup>2</sup>
                  </div>
                </div>
                <p class="vpc-error-message">
                  <span class="vpc-error-message__length">Minimale breedte is {{ section.settings.min_width }} cm en maximum is {{ section.settings.max_width }} cm.</span>
                  <span class="vpc-error-message__depth">Minimale diepte is {{ section.settings.min_depth }} cm en maximum is {{ section.settings.max_depth }} cm.</span>
                  <span class="vpc-error-message__divider"></span>
                  <span class="vpc-error-message__contact">
                    Afwijkende maten? Neem dan <a href="{{ section.settings.contact_page | default: '/pages/contact' }}" class="vpc-error-message__link">contact</a> met ons op om de mogelijkheden te bespreken!
                  </span>
                </p>
              </div>
              {% comment %} Exterior/Interior Switcher {% endcomment %}
              <ul class="vpc-switcher">
                <li class="vpc-switcher__item">
                  <input class="vpc-switcher__input" type="radio" id="vpc-exterior-button" name="vpc-view-switcher" checked required>
                  <label class="vpc-switcher__label vpc-switcher__label--exterior vpc-switcher__label--active" for="vpc-exterior-button">Buitenzijde</label>
                </li>
                <li class="vpc-switcher__item">
