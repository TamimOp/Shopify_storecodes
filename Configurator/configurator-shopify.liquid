<style>
  /* Product Selection Popup Styles */
  .vpc-product-select-popup {
    position: fixed !important;
    top: 0 !important;
    left: 0 !important;
    width: 100% !important;
    height: 100% !important;
    z-index: 99999 !important;
    display: flex !important;
    align-items: center !important;
    justify-content: center !important;
    opacity: 1 !important;
    visibility: visible !important;
    transition: opacity 0.3s ease, visibility 0.3s ease;
    background: transparent;
  }
  
  .vpc-product-select-popup.hidden {
    display: none !important;
    opacity: 0 !important;
    visibility: hidden !important;
    pointer-events: none !important;
  }
  
  .vpc-product-select-popup__overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.6);
    backdrop-filter: blur(4px);
  }
  
  .vpc-product-select-popup__inner {
    position: relative;
    background: #fff;
    border-radius: 16px;
    padding: 40px 50px 50px;
    max-width: 820px;
    width: 95%;
    box-shadow: 0 25px 80px rgba(0, 0, 0, 0.25);
    animation: popupSlideIn 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
  }
  
  @keyframes popupSlideIn {
    from {
      opacity: 0;
      transform: scale(0.9) translateY(20px);
    }
    to {
      opacity: 1;
      transform: scale(1) translateY(0);
    }
  }
  
  .vpc-product-select-popup__close {
    position: absolute;
    top: 16px;
    right: 16px;
    width: 32px;
    height: 32px;
    border: none;
    background: #f5f5f5;
    border-radius: 50%;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: background 0.2s ease, transform 0.2s ease;
  }
  
  .vpc-product-select-popup__close:hover {
    background: #e0e0e0;
    transform: rotate(90deg);
  }
  
  .vpc-product-select-popup__close svg {
    width: 18px;
    height: 18px;
    stroke: #666;
  }
  
  .vpc-product-select-popup__title {
    text-align: center;
    font-size: 28px;
    font-weight: 700;
    color: #1a1a1a;
    margin: 0 0 30px 0;
    line-height: 1.3;
  }
  
  .vpc-product-select-popup__cards {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    gap: 24px;
  }
  
  .vpc-product-select-popup__card {
    border: 2px solid #e5e5e5;
    border-radius: 12px;
    padding: 24px;
    background: #fff;
    transition: border-color 0.3s ease, box-shadow 0.3s ease, transform 0.3s ease;
    cursor: pointer;
    display: flex;
    flex-direction: column;
  }
  
  .vpc-product-select-popup__card:hover {
    border-color: #FF6B00;
    box-shadow: 0 8px 30px rgba(255, 107, 0, 0.15);
    transform: translateY(-4px);
  }
  
  .vpc-product-select-popup__card-header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 16px;
  }
  
  .vpc-product-select-popup__card-title {
    font-size: 22px;
    font-weight: 700;
    color: #1a1a1a;
    margin: 0;
  }
  
  .vpc-product-select-popup__card-badge {
    font-size: 12px;
    font-weight: 600;
    color: #FF6B00;
    background: #FFF3E8;
    padding: 4px 10px;
    border-radius: 20px;
  }
  
  .vpc-product-select-popup__card-image {
    width: 100%;
    height: 140px;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 16px;
  }
  
  .vpc-product-select-popup__card-image img {
    max-width: 100%;
    max-height: 100%;
    object-fit: contain;
  }
  
  .vpc-product-select-popup__card-description {
    font-size: 15px;
    color: #666;
    line-height: 1.5;
    margin: 0 0 20px 0;
    flex-grow: 1;
  }
  
  .vpc-product-select-popup__card-button {
    display: block;
    width: 100%;
    padding: 14px 20px;
    font-size: 16px;
    font-weight: 600;
    text-align: center;
    text-decoration: none;
    border-radius: 8px;
    border: 2px solid #FF6B00;
    background: #fff;
    color: #FF6B00;
    cursor: pointer;
    transition: all 0.3s ease;
  }
  
  .vpc-product-select-popup__card-button:hover {
    background: #FF6B00;
    color: #fff;
  }
  
  .vpc-product-select-popup__card--primary .vpc-product-select-popup__card-button {
    background: #FF6B00;
    color: #fff;
  }
  
  .vpc-product-select-popup__card--primary .vpc-product-select-popup__card-button:hover {
    background: #e55f00;
    border-color: #e55f00;
  }
  
  /* Mobile Responsive */
  @media screen and (max-width: 700px) {
    .vpc-product-select-popup__inner {
      padding: 30px 20px 35px;
      margin: 15px;
    }
    
    .vpc-product-select-popup__title {
      font-size: 22px;
      margin-bottom: 24px;
    }
    
    .vpc-product-select-popup__cards {
      grid-template-columns: 1fr;
      gap: 16px;
    }
    
    .vpc-product-select-popup__card {
      padding: 20px;
    }
    
    .vpc-product-select-popup__card-title {
      font-size: 18px;
    }
    
    .vpc-product-select-popup__card-image {
      height: 100px;
    }
    
    .vpc-product-select-popup__card-description {
      font-size: 14px;
    }
  }
</style>

<div id="vpc-product-select-popup" class="vpc-product-select-popup">
  <div class="vpc-product-select-popup__overlay" onclick="VPCProductSelect.close()"></div>
  <div class="vpc-product-select-popup__inner">
    <button type="button" class="vpc-product-select-popup__close" onclick="VPCProductSelect.close()">
      <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5" stroke-linecap="round">
        <line x1="18" y1="6" x2="6" y2="18"></line>
        <line x1="6" y1="6" x2="18" y2="18"></line>
      </svg>
    </button>
    
    <h2 class="vpc-product-select-popup__title">Selecteer je aanbouw</h2>
    
    <div class="vpc-product-select-popup__cards">
      <!-- Aanbouw Card -->
      <div class="vpc-product-select-popup__card vpc-product-select-popup__card--primary" onclick="VPCProductSelect.selectAanbouw()">
        <div class="vpc-product-select-popup__card-header">
          <h3 class="vpc-product-select-popup__card-title">Aanbouw</h3>
        </div>
        <div class="vpc-product-select-popup__card-image">
          <img src="https://cdn.shopify.com/s/files/1/0970/8562/0607/files/aanbouw-icon.png?v=1769144566" alt="Aanbouw" loading="eager">
        </div>
        <p class="vpc-product-select-popup__card-description">Geschikt voor alle<br>type huizen.</p>
        <button type="button" class="vpc-product-select-popup__card-button">Ontwerp je aanbouw</button>
      </div>
      
      <!-- Woonserre Card -->
      <div class="vpc-product-select-popup__card" onclick="VPCProductSelect.selectWoonserre()">
        <div class="vpc-product-select-popup__card-header">
          <h3 class="vpc-product-select-popup__card-title">Woonserre</h3>
          <span class="vpc-product-select-popup__card-badge">Nieuw</span>
        </div>
        <div class="vpc-product-select-popup__card-image">
          <img src="https://cdn.shopify.com/s/files/1/0970/8562/0607/files/woonserre-icon.png?v=1769144567" alt="Woonserre" loading="eager">
        </div>
        <p class="vpc-product-select-popup__card-description">Geschikt voor huizen met 1 of 2<br>vrijstaande zijgevel(s).</p>
        <button type="button" class="vpc-product-select-popup__card-button">Ontwerp je woonserre</button>
      </div>
    </div>
  </div>
</div>

<script>
  // Product Selection Popup Controller
  const VPCProductSelect = {
    popup: null,
    
    init: function() {
      this.popup = document.getElementById('vpc-product-select-popup');
      
      if (this.popup) {
        // Always show popup on page load
        this.popup.style.display = 'flex';
        this.popup.classList.remove('hidden');
        document.body.style.overflow = 'hidden';
      }
    },
    
    close: function() {
      if (this.popup) {
        this.popup.style.display = 'none';
        this.popup.classList.add('hidden');
        document.body.style.overflow = '';
      }
    },
    
    selectAanbouw: function() {
      // Close popup and stay on current page
      this.close();
    },
    
    selectWoonserre: function() {
      // Redirect to woonserre configurator
      window.location.href = '/pages/configurator-woonserre';
    }
  };
  
  // Initialize when DOM is ready
  if (document.readyState === 'loading') {
    document.addEventListener('DOMContentLoaded', function() {
      VPCProductSelect.init();
    });
  } else {
    VPCProductSelect.init();
  }
</script>

