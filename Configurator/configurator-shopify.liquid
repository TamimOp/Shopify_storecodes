                         alt="Uitbouw buitenaanzicht" 
                         loading="lazy"
                         srcset="https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_BASIS_05-05-2020_Exterieur-1-2560x1920.jpg 2560w, https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_BASIS_05-05-2020_Exterieur-1-1800x1350.jpg 1800w, https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_BASIS_05-05-2020_Exterieur-1-1400x1050.jpg 1400w, https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_BASIS_05-05-2020_Exterieur-1-1280x960.jpg 1280w, https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_BASIS_05-05-2020_Exterieur-1-980x735.jpg 980w, https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_BASIS_05-05-2020_Exterieur-1-480x360.jpg 480w"
                         sizes="(min-width: 1920px) 2560px, (min-width: 1440px) 1800px, (min-width: 1280px) 1400px, (min-width: 980px) 1280px, (min-width: 576px) 980px, 480px">
                  {% endif %}
                </div>
                {% comment %} Dynamic option images will be added here via JavaScript {% endcomment %}
              </div>

              {% comment %} Interior Preview Images {% endcomment %}
              <div class="s-configurator-container__interior-images" style="display: none;">
                {% comment %} Base interior image {% endcomment %}
                <div class="c-image c-image--base interior-element" style="z-index: 0">
                  {% if section.settings.base_interior_image %}
                    <img src="{{ section.settings.base_interior_image | image_url: width: 1200 }}" 
                         alt="Uitbouw binnenaanzicht" 
                         loading="lazy">
                  {% else %}
                    <img src="https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_BASIS_05-05-2020_Interieur-1-1280x960.jpg" 
                         alt="Uitbouw binnenaanzicht" 
                         loading="lazy"
                         srcset="https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_BASIS_05-05-2020_Interieur-1-2560x1920.jpg 2560w, https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_BASIS_05-05-2020_Interieur-1-1800x1350.jpg 1800w, https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_BASIS_05-05-2020_Interieur-1-1400x1050.jpg 1400w, https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_BASIS_05-05-2020_Interieur-1-1280x960.jpg 1280w, https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_BASIS_05-05-2020_Interieur-1-980x735.jpg 980w, https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_BASIS_05-05-2020_Interieur-1-480x360.jpg 480w"
                         sizes="(min-width: 1920px) 2560px, (min-width: 1440px) 1800px, (min-width: 1280px) 1400px, (min-width: 980px) 1280px, (min-width: 576px) 980px, 480px">
                  {% endif %}
                </div>
                
                {% comment %} Pre-populated interior option images - Kozijn Interior (default: openslaande deuren wit kunststof) {% endcomment %}
                <div class="c-image interior-element option-kozijn-interior-1" style="z-index: 15;">
                  <img class="c-image__img" alt="Kozijn interieur" data-index="15" 
                       src="https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_TRANSP_Kozijn_Kunststof_Wit_Draai_INTERIEUR-1-1280x960.png"
                       srcset="https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_TRANSP_Kozijn_Kunststof_Wit_Draai_INTERIEUR-1-2560x1920.png 2560w, https://deprefabriek.nl/wp-content/uploads/2020/03/20.269_TRANSP_Kozijn_Kunststof_Wit_Draai_INTERIEUR-1-1280x960.png 1280w"
                       sizes="(min-width: 1280px) 1280px, 100vw">
                </div>
                
                {% comment %} Stucwerk option image (default: geen stucwerk, gipsplaat) {% endcomment %}
                <div class="c-image interior-element option-stucwerk-1" style="z-index: 1;">
                  <img class="c-image__img" alt="Gipsplaat" data-index="1" 
                       src="https://deprefabriek.nl/wp-content/uploads/2020/11/20.269_gipsplaat_Donker_TRANSP-1280x960.png"
                       srcset="https://deprefabriek.nl/wp-content/uploads/2020/11/20.269_gipsplaat_Donker_TRANSP-2560x1920.png 2560w, https://deprefabriek.nl/wp-content/uploads/2020/11/20.269_gipsplaat_Donker_TRANSP-1280x960.png 1280w"
                       sizes="(min-width: 1280px) 1280px, 100vw">
                </div>
                
                {% comment %} Dynamic option images will be added/updated here via JavaScript {% endcomment %}
              </div>

            </div>
            
            {% comment %} Help Button - Outside floating banner {% endcomment %}
            <div class="vpc-price-help">
              <button type="button" class="vpc-help-button">
                <span class="vpc-help-icon">?</span>
                <span class="vpc-help-text">Hulp bij de configurator?</span>
              </button>
            </div>
            
            {% comment %} Floating Price Banner {% endcomment %}
            <div id="vpc-price-container" class="vpc-price-container">
              <div class="vpc-price-center">
                <span class="vpc-price-label">Totaal aanbouw (incl. btw)</span>
                <span id="vpc-price" class="vpc-price-value">€39.950</span>
              </div>
              <div class="vpc-price-link">
                <span class="vpc-value-text">Waardevermeerdering weten?</span>
                <button type="button" class="vpc-value-check-btn" onclick="VPCConfigurator.openValuePopup()">Check</button>
              </div>
              <div class="vpc-price-buttons">
                <button type="button" class="vpc-prev-step-btn" onclick="VPCConfigurator.goToPreviousStep()" style="display: none;">
                  Terug
                </button>
                <button type="button" class="c-button c-button--box c-button--box-colorone vpc-next-step-btn" onclick="VPCConfigurator.goToNextStep()">
                  Volgende stap
                </button>
              </div>
              <input type="hidden" id="vpc-total-price-result" value="€39.950" name="Total price" required>
            </div>
            
            {% comment %} View Switcher - Exterior/Interior {% endcomment %}
            <div class="vpc-preview__switcher">
              <button type="button" class="vpc-preview__switcher-btn vpc-preview__switcher-btn--exterior active" data-view="exterior" onclick="VPCConfigurator.switchView('exterior')">
                Buitenzijde
              </button>
              <button type="button" class="vpc-preview__switcher-btn vpc-preview__switcher-btn--interior" data-view="interior" onclick="VPCConfigurator.switchView('interior')">
                Binnenzijde
              </button>
            </div>
          </div>
        </div>

      </div>
    </div>

  </div>
</section>

<script src="{{ 'configurator.js' | asset_url }}" defer></script>

{% schema %}
{
  "name": "Product Configurator",
  "tag": "section",
  "class": "section-configurator",
  "settings": [
    {
      "type": "header",
      "content": "Dimensions"
    },
    {
      "type": "number",
      "id": "min_depth",
      "label": "Minimum Depth (cm)",
      "default": 150
    },
    {
      "type": "number",
      "id": "max_depth",
      "label": "Maximum Depth (cm)",
      "default": 400
    },
    {
      "type": "number",
      "id": "min_width",
      "label": "Minimum Width (cm)",
      "default": 200
    },
    {
      "type": "number",
      "id": "max_width",
      "label": "Maximum Width (cm)",
      "default": 1500
    },
    {
      "type": "number",
      "id": "default_depth",
      "label": "Default Depth (cm)",
      "default": 200
    },
    {
      "type": "number",
      "id": "default_width",
      "label": "Default Width (cm)",
      "default": 450
    },
    {
      "type": "header",
      "content": "Preview Images"
    },
    {
      "type": "image_picker",
      "id": "base_exterior_image",
      "label": "Base Exterior Image"
    },
    {
      "type": "image_picker",
      "id": "base_interior_image",
      "label": "Base Interior Image"
    },
    {
      "type": "header",
      "content": "Help Popup"
    },
    {
      "type": "checkbox",
      "id": "show_help_button",
      "label": "Show Help Button",
      "default": true
    },
    {
      "type": "header",
      "content": "Links"
    },
    {
      "type": "url",
      "id": "privacy_page",
      "label": "Privacy Policy Page"
    },
    {
      "type": "url",
      "id": "contact_page",
      "label": "Contact Page Link"
    }
  ],
  "presets": [
    {
      "name": "Product Configurator",
      "category": "Custom"
    }
  ]
}
{% endschema %}
