{% schema %}
{
  "name": "Product Configurator",
  "tag": "section",
  "class": "section-configurator",
  "settings": [
    {
      "type": "header",
      "content": "General Settings"
    },
    {
      "type": "text",
      "id": "heading",
      "label": "Title",
      "default": "Ontwerp jouw aanbouw"
    },
    {
      "type": "richtext",
      "id": "description",
      "label": "Description",
      "default": "<p>Configureer jouw perfecte uitbouw met onze online tool.</p>"
    },
    {
      "type": "header",
      "content": "Dimensions"
    },
    {
      "type": "number",
      "id": "min_depth",
      "label": "Minimum Depth (cm)",
      "default": 150
    },
    {
      "type": "number",
      "id": "max_depth",
      "label": "Maximum Depth (cm)",
      "default": 400
    },
    {
      "type": "number",
      "id": "min_width",
      "label": "Minimum Width (cm)",
      "default": 200
    },
    {
      "type": "number",
      "id": "max_width",
      "label": "Maximum Width (cm)",
      "default": 1500
    },
    {
      "type": "number",
      "id": "default_depth",
      "label": "Default Depth (cm)",
      "default": 200
    },
